{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python-build"
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ]
  }